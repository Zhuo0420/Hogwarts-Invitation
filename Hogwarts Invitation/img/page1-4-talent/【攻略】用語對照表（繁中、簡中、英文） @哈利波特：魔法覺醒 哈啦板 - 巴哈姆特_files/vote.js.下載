(function($,window){var Vote={vote:function(area,dataId,optionSn,callback){let csrf=new Bahamut.Csrf;csrf.setCookie();let formData=new FormData;formData.append("option_sn",optionSn);formData.append("area",area);formData.append("data_id",dataId);fetch("https://api.gamer.com.tw/vote/v1/vote_toggle.php",{method:"post",body:formData,headers:csrf.getFetchHeaders(),credentials:"include"}).then(resp=>{resp.json().then(json=>{callback(json)})})},getOptionUserList:function(area,dataId,optionSn,page,callback){let formData=new FormData;formData.append("option_sn",optionSn);formData.append("area",area);formData.append("data_id",dataId);formData.append("page",page);url=new URL("https://api.gamer.com.tw/vote/v1/vote_user_list.php");url.search=new URLSearchParams(formData).toString();fetch(url,{method:"get",credentials:"include"}).then(resp=>{resp.json().then(json=>{callback(json)})})},getVote:function(area,dataId,callback){let formData=new FormData;formData.append("area",area);formData.append("data_id",dataId);url=new URL("https://api.gamer.com.tw/vote/v1/vote_get.php");url.search=new URLSearchParams(formData).toString();fetch(url,{method:"get",credentials:"include"}).then(resp=>{resp.json().then(json=>{callback(json)})})}};window.Vote=Vote})(jQuery,window);