!function(e,o,t){var a=e.DFP||{};a.setcookie=function(e){return"undefined"==typeof Cookies?!1:void o.ajax({url:"https://api.gamer.com.tw/buap/index.php",method:"GET",dataType:"json",data:e}).done(function(e){return e.error!==t?!1:e.data===t||e.data.targeting===t?!1:void o.each(e.data.targeting,function(e,o){o.length>0?Cookies.set(e,o,{expires:14,domain:".gamer.com.tw"}):Cookies.remove(e,{domain:".gamer.com.tw"})})})},a.setTargeting=function(){if("undefined"==typeof googletag||"undefined"==typeof Cookies)return!1;var e=Cookies.get("buap_modr")||"";e.length>0&&googletag.pubads().setTargeting("modr",e);var o=Cookies.get("buap_puoo")||"";o.length>0&&googletag.pubads().setTargeting("puoo",o);var t=Cookies.get("buap_adLevel")||"";t.length>0&&googletag.pubads().setTargeting("adLevel",t)},e.DFP=a}(window,jQuery);